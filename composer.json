{
    "name": "juzaweb/referral",
    "type": "juzaweb-plugin",
    "license": "MIT",
    "description": "Referral support for Juzaweb",
    "extra": {
        "juzaweb": {
            "name": "Referral",
            "domain": "referral",
            "providers": [
                "Juzaweb\\Referral\\Providers\\ReferralServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Juzaweb\\Referral\\": "src/"
        },
        "files": [
            "helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Juzaweb\\Referral\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.0|^8.1"
    },
    "require-dev": {
         "juzaweb/modules": "^1.0",
         "juzaweb/dev-tool": "^1.0"
    }
}
